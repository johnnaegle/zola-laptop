alias wtf-ssh='killall -m ssh-agent; eval `ssh-agent` && ssh-add -K ~/.ssh/id_rsa'
alias v='cd ~/web/tools/m1 && vagrant'
alias restart-web="pushd ~/web/web-marketplace && npm run dev-stop && npm run dev-staging && popd"
